# API Keys (Never commit the actual .env file)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=True

# LLM Configuration
DEFAULT_LLM_PROVIDER=openai
DEFAULT_MODEL=gpt-4-turbo-preview
MAX_TOKENS=4096
TEMPERATURE=0.7

# Agent Configuration
ENABLE_RECORDS_WRANGLER=True
ENABLE_CLIENT_COMMUNICATION=True
ENABLE_LEGAL_RESEARCHER=True
ENABLE_VOICE_BOT_SCHEDULER=True
ENABLE_EVIDENCE_SORTER=True

# Twilio Configuration (for voice calling)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+15551234567
ATTORNEY_PHONE_NUMBER=+15559876543

# Call Recording Configuration
ENABLE_CALL_RECORDING=True
CALL_RECORDING_STATUS_CALLBACK_URL=http://localhost:8000/api/calls/recording-callback
